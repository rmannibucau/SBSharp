<!DOCTYPE html>
<html lang="en-US" class="h-100">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <meta name='robots' content='index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1' />
    @RenderSection("Meta", false)

    <title>@Model.Document.Header.Title</title>

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.10.0/styles/github-dark.min.css"
        integrity="sha512-rO+olRTkcf304DQBxSWxln8JXCzTHlKnIdnMUwYvQa9/Jd4cQaNkItIUj6Z4nvW1dqK0SKXLbn9h4KwZTNtAyw=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
        integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
    <style>
        dl dd {
            margin-left: 1.125em;
        }

        dl dd {
            margin-bottom: 1.25em;
        }

        blockquote,
        dt,
        p,
        span.alt,
        td.content {
            font-size: 1.0625rem;
        }

        pre {
            font-size: 1rem;
        }

        p {
            margin-bottom: 1rem;
        }

        div.colist.arabic>ol {
            list-style: none;
            padding-left: 0;
            counter-reset: custom-counter;
        }

        div.colist.arabic>ol>li {
            counter-increment: custom-counter;
        }

        div.colist.arabic>ol>li::before {
            content: '( ' counter(custom-counter) ' )';
        }
    </style>
    @RenderSection("Styles", false)
</head>

<body class="d-flex flex-column h-100" cz-shortcut-listen="true">
    <header class="flex-shrink-0" style="border-bottom: 3px solid #0090cd;">
        <div class="container pe-3">
            <nav class="navbar navbar-expand-sm" aria-label="RManniBucau.NET">
                <div class="container-fluid">
                    <a class="navbar-brand" href="/">RManniBucau.NET</a>
                    <button class="navbar-toggler collapsed mb-2" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarsExample03" aria-controls="navbarsExample03" aria-expanded="false"
                        aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>

                    <div class="navbar-collapse collapse" id="navbarsExample03">
                        <span class="me-auto mb-2 mb-sm-0"></span>
                        <div class="text-end d-flex justify-content-center">
                            <ul class="navbar-nav flex-row flex-wrap">
                                <li class="nav-item col-6 col-lg-auto">
                                    <a class="nav-link py-2 px-0 px-lg-2" href="https://github.com/rmannibucau"
                                        target="_blank" rel="noopener" style="color: #0090cd">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                                            class="navbar-nav-svg" viewBox="0 0 512 499.36" role="img">
                                            <title>GitHub</title>
                                            <path fill="#0090cd" fill-rule="evenodd"
                                                d="M256 0C114.64 0 0 114.61 0 256c0 113.09 73.34 209 175.08 242.9 12.8 2.35 17.47-5.56 17.47-12.34 0-6.08-.22-22.18-.35-43.54-71.2 15.49-86.2-34.34-86.2-34.34-11.64-29.57-28.42-37.45-28.42-37.45-23.27-15.84 1.73-15.55 1.73-15.55 25.69 1.81 39.21 26.38 39.21 26.38 22.84 39.12 59.92 27.82 74.5 21.27 2.33-16.54 8.94-27.82 16.25-34.22-56.84-6.43-116.6-28.43-116.6-126.49 0-27.95 10-50.8 26.35-68.69-2.63-6.48-11.42-32.5 2.51-67.75 0 0 21.49-6.88 70.4 26.24a242.65 242.65 0 0 1 128.18 0c48.87-33.13 70.33-26.24 70.33-26.24 14 35.25 5.18 61.27 2.55 67.75 16.41 17.9 26.31 40.75 26.31 68.69 0 98.35-59.85 120-116.88 126.32 9.19 7.9 17.38 23.53 17.38 47.41 0 34.22-.31 61.83-.31 70.23 0 6.85 4.61 14.81 17.6 12.31C438.72 464.97 512 369.08 512 256.02 512 114.62 397.37 0 256 0z">
                                            </path>
                                        </svg>
                                    </a>
                                </li>
                                <li class="nav-item col-6 col-lg-auto">
                                    <a class="nav-link py-2 px-1 px-lg-2" href="https://twitter.com/rmannibucau"
                                        target="_blank" rel="noopener" style="color: #0090cd">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24"
                                            class="navbar-nav-svg" viewBox="0 0 512 416.32" role="img">
                                            <title>Twitter</title>
                                            <path fill="#0090cd"
                                                d="M160.83 416.32c193.2 0 298.92-160.22 298.92-298.92 0-4.51 0-9-.2-13.52A214 214 0 0 0 512 49.38a212.93 212.93 0 0 1-60.44 16.6 105.7 105.7 0 0 0 46.3-58.19 209 209 0 0 1-66.79 25.37 105.09 105.09 0 0 0-181.73 71.91 116.12 116.12 0 0 0 2.66 24c-87.28-4.3-164.73-46.3-216.56-109.82A105.48 105.48 0 0 0 68 159.6a106.27 106.27 0 0 1-47.53-13.11v1.43a105.28 105.28 0 0 0 84.21 103.06 105.67 105.67 0 0 1-47.33 1.84 105.06 105.06 0 0 0 98.14 72.94A210.72 210.72 0 0 1 25 370.84a202.17 202.17 0 0 1-25-1.43 298.85 298.85 0 0 0 160.83 46.92">
                                            </path>
                                        </svg>
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </nav>
        </div>
    </header>

    <main class="flex-shrink-1 mb-auto" style="background-color: #f1f1f1; flex: 1;">
        @RenderBody()
    </main>

    <footer class="footer bg-light flex-shrink-0">
        <div class="container py-2 py-md-2 px-2 px-md-2 text-body-secondary">
            <div class="row">
                <div class="col-lg-3 mb-1">
                    <a class="d-inline-flex align-items-center mb-2 text-body-emphasis text-decoration-none" href="#"
                        aria-label="RManniBucau > SBSharp">
                        RManniBucau :: SBSharp
                    </a>
                    <ul class="list-unstyled small">
                        <li class="mb-2">A .NET static site platform</li>
                    </ul>
                </div>
                <div class="col-6 col-lg-2 offset-lg-1 mb-1">
                    <h5>Blogs</h5>
                    <ul class="list-unstyled">
                        <li class="mb-2"><a href="https://rsharp.github.com" target="_blank">R#</a></li>
                        <li class="mb-2"><a href="https://rmannibucau.metawerx.net/" target="_blank">General Blog</a>
                        </li>
                    </ul>
                </div>
                <div class="col-6 col-lg-2 mb-1">
                    <h5>Social Links</h5>
                    <ul class="list-unstyled">
                        <li class="mb-2">
                            <a class="no-underline" title="Twitter" data-bi-id="author follow within post"
                                data-bi-area="" data-bi-name="twitter" aria-label="Bruno Capuano Twitter profile"
                                target="_blank" href="https://www.twitter.com/elbruno">
                                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="18" class="s-twitter-x"
                                    fill="#666" viewBox="0 0 512 512">
                                    <path
                                        d="M389.2 48h70.6L305.6 224.2 487 464H345L233.7 318.6 106.5 464H35.8L200.7 275.5 26.8 48H172.4L272.9 180.9 389.2 48zM364.4 421.8h39.1L151.1 88h-42L364.4 421.8z">
                                    </path>
                                </svg>
                            </a>

                        </li>
                        <li class="mb-2">
                            <a class="no-underline" title="Github" data-bi-id="author follow within post"
                                data-bi-area="" data-bi-name="git_profile_url"
                                aria-label="Romain Manni-Bucau Github profile" target="_blank"
                                href="https://github.com/rmannibucau">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" class="navbar-nav-svg"
                                    viewBox="0 0 512 499.36" role="img">
                                    <title>GitHub</title>
                                    <path fill="#666" fill-rule="evenodd"
                                        d="M256 0C114.64 0 0 114.61 0 256c0 113.09 73.34 209 175.08 242.9 12.8 2.35 17.47-5.56 17.47-12.34 0-6.08-.22-22.18-.35-43.54-71.2 15.49-86.2-34.34-86.2-34.34-11.64-29.57-28.42-37.45-28.42-37.45-23.27-15.84 1.73-15.55 1.73-15.55 25.69 1.81 39.21 26.38 39.21 26.38 22.84 39.12 59.92 27.82 74.5 21.27 2.33-16.54 8.94-27.82 16.25-34.22-56.84-6.43-116.6-28.43-116.6-126.49 0-27.95 10-50.8 26.35-68.69-2.63-6.48-11.42-32.5 2.51-67.75 0 0 21.49-6.88 70.4 26.24a242.65 242.65 0 0 1 128.18 0c48.87-33.13 70.33-26.24 70.33-26.24 14 35.25 5.18 61.27 2.55 67.75 16.41 17.9 26.31 40.75 26.31 68.69 0 98.35-59.85 120-116.88 126.32 9.19 7.9 17.38 23.53 17.38 47.41 0 34.22-.31 61.83-.31 70.23 0 6.85 4.61 14.81 17.6 12.31C438.72 464.97 512 369.08 512 256.02 512 114.62 397.37 0 256 0z">
                                    </path>
                                </svg>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
        crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/11.10.0/highlight.min.js"
        integrity="sha512-6yoqbrcLAHDWAdQmiRlHG4+m0g/CT/V9AGyxabG8j7Jk8j3r3K6due7oqpiRMZqcYe9WM2gPcaNNxnl2ux+3tA=="
        crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script>hljs.highlightAll();</script>
    @RenderSection("Scripts", false)
    @if (Environment.GetEnvironmentVariable("DOC_CMD") == "serve")
    {
        <script src="https://cdn.jsdelivr.net/gh/Q42/LiveJS@f74924ba568ccefb953ee74120213bc3c21e0125/livejs.js"></script>
    }
</body>

</html>